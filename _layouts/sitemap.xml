---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url | append: site.baseurl | append: '/' }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1.0</priority>
  </url>
  {% for page in site.pages %}
  <url>
    <loc>{{ site.url }}{{ page.url | replace:'index.html','' }}</loc>
    <lastmod>{{ page.date | xmlschema }}</lastmod>
    <changefreq>{{ page.changefreq | default: site.sitemap.changefreq }}</changefreq>
    <priority>{{ page.priority | default: site.sitemap.priority }}</priority>
  </url>
  {% endfor %}
</urlset>
